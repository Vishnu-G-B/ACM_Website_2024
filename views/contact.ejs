              data-wf-element-id="671f0823-c9f2-6e71-cdf7-21666e4b7951">
              <input type="text" class="moder-form-text-field w-input" maxlength="256" name="Name" data-name="Name"
                placeholder="Your Name" id="Name" required="" />
                <input type="tel" class="moder-form-text-field w-input"
                maxlength="256" name="Name-3" data-name="Name 3" placeholder="Your Phone" id="Name-3"
                required="" />
                <input type="email" class="moder-form-text-field w-input" maxlength="256" name="Name-2"
                data-name="Name 2" placeholder="Your Email" id="Name-2" required="" />
                <textarea id="Message"
                name="Message" maxlength="5000" data-name="Message" placeholder="Your Message" required=""
                class="modern-text-area w-input"></textarea>
                <input type="submit" value="Submit your Message"
                data-wait="Please wait..." class="button-contact-us w-button" />
            </form>
            <script defer>
              document.getElementById("wf-form-Contact-us-Email-Form").addEventListener("submit",function(event) {
                event.preventDefault();
                const form = event.target;
                const formData = new FormData(form);
                const subject = 'Contact form submission';
                let body = "Name: " + formData.get('Name') + "\n";
                body += "Phone: " + formData.get('Name-3') + "\n";
                body += "Email: " + formData.get('Name-2') + "\n";
                body += "Message: " + formData.get('Message');
                const mailToURL = "mailto:acm.mitblr@manipal.edu?subject=" + encodeURIComponent(subject) + "&body=" + encodeURIComponent(body);
                window.location.href = mailToURL;
              })
            </script>
            <div class="success-message w-form-done">
